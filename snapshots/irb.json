[
  {
    "name": "irb",
    "options": [
      {
        "type": "long",
        "key": "version",
        "title": "--version",
        "description": "Prints the version of irb."
      },
      {
        "type": "short",
        "key": "E",
        "title": "-E external[:internal], --encoding external[:internal]",
        "description": "Same as `ruby -E' . Specifies the default value(s) for external encodings and internal encoding. Values should be separated with colon (:).\n\nYou can omit the one for internal encodings, then the value (Encoding.default_internal) will be nil."
      },
      {
        "type": "long",
        "key": "encoding",
        "title": "-E external[:internal], --encoding external[:internal]",
        "description": "Same as `ruby -E' . Specifies the default value(s) for external encodings and internal encoding. Values should be separated with colon (:).\n\nYou can omit the one for internal encodings, then the value (Encoding.default_internal) will be nil."
      },
      {
        "type": "short",
        "key": "I",
        "title": "-I path",
        "description": "Same as `ruby -I' . Specifies $LOAD_PATH directory"
      },
      {
        "type": "short",
        "key": "U",
        "title": "-U",
        "description": "Same as `ruby -U' . Sets the default value for internal encodings (Encoding.default_internal) to UTF-8."
      },
      {
        "type": "short",
        "key": "d",
        "title": "-d",
        "description": "Same as `ruby -d' . Sets $DEBUG to true."
      },
      {
        "type": "short",
        "key": "f",
        "title": "-f",
        "description": "Suppresses read of ~/.irbrc."
      },
      {
        "type": "short",
        "key": "w",
        "title": "-w",
        "description": "Same as `ruby -w' ."
      },
      {
        "type": "short",
        "key": "W",
        "title": "-W",
        "description": "Same as `ruby -W' ."
      },
      {
        "type": "short",
        "key": "h",
        "title": "-h, --help",
        "description": "Prints a summary of the options."
      },
      {
        "type": "long",
        "key": "help",
        "title": "-h, --help",
        "description": "Prints a summary of the options."
      },
      {
        "type": "short",
        "key": "r",
        "title": "-r library",
        "description": "Same as `ruby -r'. Causes irb to load the library using require."
      },
      {
        "type": "long",
        "key": "inspect",
        "title": "--inspect",
        "description": "Uses `inspect' for output (default except for bc mode)"
